{  
  "name": "send",  
  "manifest_version":2,
  "version": "1.0.0",  
  "description": "Send password",
  "browser_action": {  
    "default_icon": "icon.png" ,
    "default_title": "Jubar",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://*",
    "https://*"
  ],
  "content_scripts":[
    {
        "matches": ["http://localhost:8080/"],
        "js": ["content.js"],
        "run_at": "document_end"
    }
]
} 